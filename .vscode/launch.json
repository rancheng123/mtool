{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron Main",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "env": {
        "NODE_ENV": "development"
      },
      "program": "${workspaceRoot}/dist/electron-main.js",
      "protocol": "inspector"
    }
  ]
}